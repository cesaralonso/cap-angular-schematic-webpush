{
  "$schema": "http://json-schema.org/schema",
  "id": "CAP Angular Schematic WebPush",
  "title": "CAP Angular Schematic WebPush",
  "type": "object",
  "properties": {
    "project": {
      "type": "string",
      "description": "App Name",
      "x-prompt": "What is the project title for the App?",
      "$default": {
        "$source": "argv",
        "index": 0
      }
    },
    "domain": {
      "type": "string",
      "description": "Domain and Port",
      "x-prompt": "What is the API domain and port for connect the web push socket?",
      "default": "http://localhost:4000",
      "$default": {
        "$source": "argv",
        "index": 1
      }
    },
    "vapidPublicKey": {
      "type": "string",
      "description": "Vapid PublicKey",
      "x-prompt": "What is the Vapid PublicKey?",
      "default": "BLnVk1MBGFBW4UxL44fuoM2xxQ4o9CuxocVzKn9UVmnXZEyPCTEFjI4sALMB8qN5ee67yZ6MeQWjd5iyS8lINAg",
      "$default": {
        "$source": "argv",
        "index": 2
      }
    },
    "vapidPrivateKey": {
      "type": "string",
      "description": "Vapid PrivateKey",
      "x-prompt": "What is the Vapid PrivateKey?",
      "default": "mp5xYHWtRTyCA63nZMvmJ_qmYO6A1klSotcoppSx-MI",
      "$default": {
        "$source": "argv",
        "index": 3
      }
    }

    
  },
  "required": ["project", "domain", "vapidPublicKey", "vapidPrivateKey"]
}